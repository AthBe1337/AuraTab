{
  "manifest_version": 3,
  "name": "AuraTab",
  "version": "0.1.0",
  "description": "Redefine your new tab experience.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "vite.svg", 
    "48": "vite.svg",
    "128": "vite.svg"
  },
  "permissions": [
    "storage", 
    "bookmarks", 
    "favicon"
  ],
  "host_permissions": [
    "https://*/*", 
    "https://devapi.qweather.com/*",
    "https://geoapi.qweather.com/*"
  ]
}